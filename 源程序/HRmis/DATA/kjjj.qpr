SELECT Kjjj.bmmc AS 部门, Kjjj.cjmc AS 职务, Kjjj.ryxm AS 姓名,;
  Kjjj.hj AS 总收入, Kjjj.pj AS 平均收入, Kjjj.jjhj AS 奖金,;
  Kjjj.jfjs AS 平均奖金, Kjjj.sfje AS 实发金额;
 FROM kjjj;
 ORDER BY Kjjj.hj DESC
