SELECT Ryzk.dwmc AS 单位, Ryzk.cjmc AS 部门, Ryzk.bmmc AS 班组,;
  Ryzk.gwfl1 AS 岗位分类, SUM(Ryzk.a) AS 人数;
 FROM ;
     ryzk;
 WHERE (  Ryzk.dwdm = ( "&dwdm1" );
   AND  Ryzk.cjdm = ( "&CJdm1" ) );
   AND  Ryzk.bmbh = ( "&BMbh1" );
 GROUP BY Ryzk.dwmc, Ryzk.cjmc, Ryzk.bmmc, Ryzk.gwfl1;
 INTO TABLE temp.dbf  
