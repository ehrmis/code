SELECT Ryzk.dwmc AS 单位, Ryzk.xb1 AS 性别,;
  ROUND(SUM(Ryzk.gl)/SUM(Ryzk.a),0) AS 平均工龄;
 FROM ;
     ryzk;
 GROUP BY Ryzk.dwmc, Ryzk.xb1;
 INTO TABLE temp.dbf  
