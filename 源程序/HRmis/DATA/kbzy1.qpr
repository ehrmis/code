SELECT Ryzk.dwmc AS 单位, Ryzk.cjmc AS 部门, Ryzk.zy AS 专业,;
  SUM(Ryzk.a) AS 人数;
 FROM ;
     ryzk;
 GROUP BY Ryzk.dwmc, Ryzk.cjmc, Ryzk.zy;
 INTO TABLE temp.dbf  
