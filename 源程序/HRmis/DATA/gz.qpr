SELECT Ryzk.dwmc AS 单位, Ryzk.cjmc AS 部门, Ryzk.bmmc AS 班组,;
  Ryzk.gz1 AS 工种, Ryzk.岗等 AS 岗位等级, SUM(Ryzk.a) AS 人数;
 FROM ;
     ryzk;
 WHERE (  Ryzk.dwdm = ( "&dwdm1" );
   AND  Ryzk.cjdm = ( "&CJdm1" ) );
   AND  Ryzk.bmbh = ( "&BMbh1" );
 GROUP BY Ryzk.dwmc, Ryzk.cjmc, Ryzk.bmmc, Ryzk.gz1, Ryzk.岗等;
 INTO TABLE temp.dbf  
